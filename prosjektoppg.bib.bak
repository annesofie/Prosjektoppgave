keywords = {Computer software maintenance,Crowd sourcing geographic data,Geographic data,Geographic data collection,Geographic information science,Geographic information systems,OSM,Quality assessment,Quality assessment model,Quality control,Quality elements,Road network,Roads and streets,Social networking (online),Transportation,Urban traffic networks},
number = {June},
