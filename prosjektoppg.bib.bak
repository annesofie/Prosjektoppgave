title = {{Quality analysis of open street map data}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84929074534{\&}partnerID=40{\&}md5=cc6cb4934bc6f9cdeea8a05303363311},
volume = {40},
